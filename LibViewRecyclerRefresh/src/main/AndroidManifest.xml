<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yline.view.recycler.refresh">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.yline.view.recycler.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.yline.view.recycler.refresh.CircleProgressActivity" />
        <activity android:name="com.yline.view.recycler.refresh.CustomRefreshActivity" />
        <activity android:name="com.yline.view.recycler.refresh.SimpleRefreshActivity" />
    </application>
</manifest>
